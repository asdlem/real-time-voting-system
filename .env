# 项目标识
PROJECT_NAME=voting-system

# 数据库配置
DB_HOST=localhost
DB_PORT=13306
DB_USER=voteuser
DB_PASSWORD=votepassword
DB_NAME=votingdb

# MySQL配置
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=votingdb
MYSQL_USER=voteuser
MYSQL_PASSWORD=votepassword

# Redis配置
REDIS_ADDR=localhost:16379
REDIS_PASSWORD=redispassword
REDIS_DB=0
REDIS_MOCK=false  # 使用真实Redis服务

# 应用配置
GIN_MODE=debug  # 开发环境设置为debug，生产环境设置为release
LOG_LEVEL=debug
ENVIRONMENT=development

# 高并发防重复配置
ENABLE_RATE_LIMIT=true
GLOBAL_RATE_LIMIT=100
USER_RATE_LIMIT=10

# 服务器配置
SERVER_PORT=8090
API_PREFIX=/api

# 前端配置
REACT_APP_API_BASE_URL=http://localhost:8090

# 消息队列配置
# 使用内存模拟消息队列
ROCKETMQ_MOCK=true

# 其他配置
# （保留原有配置） 